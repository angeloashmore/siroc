{
  "name": "siroc",
  "version": "0.1.2",
  "description": "Zero-config build tooling for Node",
  "keywords": [
    "node",
    "nodejs",
    "typescript",
    "javascript"
  ],
  "repository": "nuxt-contrib/siroc",
  "license": "MIT",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "dist/index.d.ts",
    "!**/*.map"
  ],
  "scripts": {
    "build": "siroc build",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test",
    "test": "run-s test:*",
    "test:unit": "siroc test",
    "watch": "yarn build -w"
  },
  "dependencies": {
    "@siroc/cli": "0.1.2",
    "@siroc/core": "0.1.2"
  }
}
